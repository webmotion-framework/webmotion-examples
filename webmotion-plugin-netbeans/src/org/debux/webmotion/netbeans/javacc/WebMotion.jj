{
    (
          <SECTION_CONFIG_NAME>
        | <SECTION_CONFIG_NEXT_CONFIG>
        | <SECTION_ACTIONS_NEXT_CONFIG>
        | <SECTION_ERRORS_NEXT_CONFIG>
        | <SECTION_FILTERS_NEXT_CONFIG>
