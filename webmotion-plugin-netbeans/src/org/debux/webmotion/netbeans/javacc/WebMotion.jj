|   <METHODS_HTTP: <METHOD_HTTP> ("," <METHOD_HTTP>)*>
|   <METHOD_HTTP: ("*" | "GET" | "POST" | "PUT" | "DELETE" | "HEAD")>

|   <NEW_LINE: ("\n" | "\r" | "\r\n")+>
|   <WHITESPACE: " " | "\t" | "\f">
|   <DIGIT: ["0"-"9"]>
|   <LETTER: ["a"-"z", "A"-"Z"]>
