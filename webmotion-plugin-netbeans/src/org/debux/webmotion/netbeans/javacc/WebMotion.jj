|   <CONFIG_KEY : "package.base" | "package.view">
|   <CONFIG_EQUALS : "="> : SECTION_CONFIG_VALUE
}

<SECTION_CONFIG_VALUE> TOKEN: {
    <CONFIG_VALUE: (~["\n"])+>
|   <CONFIG_END: <NEW_LINE>> : SECTION_CONFIG
}

<SECTION_ACTIONS> TOKEN: {
