void Mapping() : {}
{
    (
        SectionConfig() 
      | SectionActions() 
      | SectionErrors() 
      | SectionFilters()
      | SectionExtensions()
      | SectionProperties()
    )* <EOF>
}

void SectionConfig() : {}
{
    SectionConfigName() (SectionConfigLine())*
}

